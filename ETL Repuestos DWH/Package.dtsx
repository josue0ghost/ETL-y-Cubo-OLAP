                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT P.ID_Partes, 
			C.ID_Categoria, 
			L.ID_Linea, 
			P.Nombre,
